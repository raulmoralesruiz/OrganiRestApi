            ]
        },
        "brand": {
            "$id": "#/properties/brand",
            "type": "string",
            "minLength": 3,
            "maxLength": 50,
            "title": "The brand schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "microsoft"
            ]
        },
        "model": {
            "$id": "#/properties/model",
            "type": "string",
            "minLength": 3,
            "maxLength": 100,
